	REOD_REALIZACIONPROD	CHARACTER VARYING(20) 	NOT NULL,
	REOD_OBSERVACION	CHARACTER VARYING(20) 		,
	REOD_FECHACAMBIO	CHARACTER VARYING(10) 	NOT NULL,
	REOD_HORACAMBIO		CHARACTER VARYING(8)	NOT NULL,
	REOD_REGISTRADOPOR	CHARACTER VARYING(30)	NOT NULL,
	CONSTRAINT PK_REOD PRIMARY KEY (REOD_ID),
	CONSTRAINT FK_REOD_ODON FOREIGN KEY (ODON_ID)
		REFERENCES "HISCLINICA"."ODONTOGRAMA" (ODON_ID) MATCH SIMPLE
		ON UPDATE RESTRICT ON DELETE RESTRICT,
	CONSTRAINT FK_REOD_ESPC FOREIGN KEY (ESPC_ID)
		REFERENCES "HISCLINICA"."ESPECIFICACIONPARTECUERPO" (ESPC_ID) MATCH SIMPLE
		ON UPDATE RESTRICT ON DELETE RESTRICT,
	CONSTRAINT FK_REOD_PRTO FOREIGN KEY (PRTO_ID)
		REFERENCES "HISCLINICA"."PROCEDIMIENTOS" (PRTO_ID) MATCH SIMPLE
		ON UPDATE RESTRICT ON DELETE RESTRICT,
	CONSTRAINT FK_REOD_SECC FOREIGN KEY (SECC_ID)
		REFERENCES "HISCLINICA"."SECCIONODON" (SECC_ID) MATCH SIMPLE
		ON UPDATE RESTRICT ON DELETE RESTRICT
);


