	CONSTRAINT PK_SECC PRIMARY KEY (SECC_ID)
);

CREATE TABLE "HISCLINICA"."REGISTROODONTOGRAMA"
(
	REOD_ID 		NUMERIC(30,0)		NOT NULL,
	ODON_ID 		NUMERIC(30,0)		NOT NULL,
	ESPC_ID 		NUMERIC(30,0)		NOT NULL,
	PRTO_ID 		NUMERIC(30,0)		NOT NULL,
