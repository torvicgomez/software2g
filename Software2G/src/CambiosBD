CREATE TABLE "HISCLINICA"."PARENTESCO"
(
	PARE_ID 		NUMERIC(30,0)		NOT NULL,
	PARE_PARENTESCO		CHARACTER VARYING(30)	NOT NULL,
	PARE_ABREVIATURA	CHARACTER VARYING(10)		,
	PARE_FECHACAMBIO	CHARACTER VARYING(10) 	NOT NULL,
	PARE_HORACAMBIO		CHARACTER VARYING(8)	NOT NULL,
	PARE_REGISTRADOPOR	CHARACTER VARYING(30)	NOT NULL,
	CONSTRAINT PK_PARE PRIMARY KEY (PARE_ID)
);

CREATE TABLE "HISCLINICA"."TIPOSERVICIO"
(
	TISE_ID 		NUMERIC(30,0)		NOT NULL,
	TISE_NOMBRE		CHARACTER VARYING(50)	NOT NULL,
	TISE_CODIGO		CHARACTER VARYING(10)	NOT NULL,
	TISE_DESCRIPCION	CHARACTER VARYING(100)		,
	TISE_FECHACAMBIO	CHARACTER VARYING(10) 	NOT NULL,
	TISE_HORACAMBIO		CHARACTER VARYING(8)	NOT NULL,
	TISE_REGISTRADOPOR	CHARACTER VARYING(30)	NOT NULL,
	CONSTRAINT PK_TISE PRIMARY KEY (TISE_ID)
);

CREATE TABLE "HISCLINICA"."MOTIVO"
(
	MOTI_ID 		NUMERIC(30,0)		NOT NULL,
	MOTI_MOTIVO		CHARACTER VARYING(50)	NOT NULL,
	MOTI_CODIGO		CHARACTER VARYING(10)	NOT NULL,
	MOTI_FECHACAMBIO	CHARACTER VARYING(10) 	NOT NULL,
	MOTI_HORACAMBIO		CHARACTER VARYING(8)	NOT NULL,
	MOTI_REGISTRADOPOR	CHARACTER VARYING(30)	NOT NULL,
	CONSTRAINT PK_MOTI PRIMARY KEY (MOTI_ID)
);

